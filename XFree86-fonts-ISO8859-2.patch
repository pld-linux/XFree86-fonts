--- XFree86-fonts-4.0.2/ulT1mo-beta-1.0/Makefile	Thu Jan  1 01:00:00 1970
+++ XFree86-fonts-4.0.2.new/ulT1mo-beta-1.0/Makefile	Sat Dec 23 21:20:54 2000
@@ -0,0 +1,24 @@
+FONTDIR=/usr/share/fonts
+
+all: 
+	for j in *.pfb ; do \
+	   mv $$j `basename $$j .pfb`-ISO-8859-2.pfb; \
+	done; \
+	cd afm; \
+	for j in *.afm ; do \
+	   mv $$j `basename $$j .afm`-ISO-8859-2.afm; \
+	done; \
+	cd ../pfm; \
+	for j in *.pfm ; do \
+	   mv $$j `basename $$j .pfm`-ISO-8859-2.pfm; \
+	done;
+
+install:
+	install -d $(FONTDIR)/Type1
+	install -d $(FONTDIR)/Type1/afm
+	install -d $(FONTDIR)/Type1/pfm
+	install *.pfb $(FONTDIR)/Type1
+	install afm/*.afm $(FONTDIR)/Type1/afm
+	install pfm/*.pfm $(FONTDIR)/Type1/pfm
+
+
